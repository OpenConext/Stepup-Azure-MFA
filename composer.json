{
  "name": "surfnet/stepup-azure-mfa",
  "license": "Apache-2.0",
  "description": "GSSP for Microsoft Azure Multi-factor authentication",
  "type": "project",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Surfnet\\": "src\\Surfnet"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dev\\": "dev",
      "Surfnet\\AzureMfa\\Test\\Features\\": "tests\\Functional\\Features",
      "Surfnet\\AzureMfa\\Test\\Unit\\": "tests\\Unit",
      "Surfnet\\AzureMfa\\Test\\WebTest\\": "tests\\Functional\\WebTests"
    }
  },
  "require": {
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-zlib": "*",
    "incenteev/composer-parameter-handler": "^2.1.5",
    "openconext/monitor-bundle": "^2.1",
    "sensio/framework-extra-bundle": "^5.5.7",
    "surfnet/stepup-bundle": "^4.2.6",
    "surfnet/stepup-gssp-bundle": "^4.0.2",
    "surfnet/stepup-saml-bundle": "^4.4.3",
    "symfony/asset": "^4.4.46",
    "symfony/config": "4.4.*",
    "symfony/console": "4.4.*",
    "symfony/dependency-injection": "4.4.*",
    "symfony/dotenv": "4.4.*",
    "symfony/flex": "^1.20.2",
    "symfony/form": "^4.4.48",
    "symfony/monolog-bundle": "^3.8",
    "symfony/security-bundle": "^4.4.50",
    "symfony/templating": "^4.4.44",
    "symfony/translation": "^4.4.47",
    "symfony/twig-bundle": "^4.4.41",
    "symfony/validator": "^4.4.48",
    "symfony/webpack-encore-bundle": "^1.17.1",
    "symfony/yaml": "^4.4.45"
  },
  "require-dev": {
    "behat/mink": "^1.10",
    "behat/mink-browserkit-driver": "^1.4.1",
    "behat/mink-goutte-driver": "^1.3",
    "behat/symfony2-extension": "^2.1.5",
    "behatch/contexts": "^3.3",
    "dbrekelmans/bdi": "^0.2",
    "jakub-onderka/php-parallel-lint": "~1",
    "malukenho/docheader": "^0.1.8",
    "mockery/mockery": "^1.3.6",
    "phpmd/phpmd": "^2.13",
    "phpunit/phpcov": "^6.0.1",
    "sebastian/phpcpd": "^4.1",
    "squizlabs/php_codesniffer": "^3.7.2",
    "symfony/browser-kit": "^4.4.44",
    "symfony/css-selector": "4.4.*",
    "symfony/maker-bundle": "^1.39.1",
    "symfony/panther": "^0.9",
    "symfony/phpunit-bridge": "^5.4.26",
    "symfony/stopwatch": "4.4.*",
    "symfony/web-profiler-bundle": "4.4.*"
  },
  "scripts": {
    "check": [
      "@composer-validate",
      "@docheader",
      "@lint",
      "@phpcpd",
      "@phpcs",
      "@phpmd",
      "@phpstan"
    ],

    "behat": "./ci/qa/behat",
    "composer-validate": "./ci/qa/validate",
    "docheader": "./ci/qa/docheader",
    "lint": "./ci/qa/lint",
    "phpcs": "./ci/qa/phpcs",
    "phpcpd": "./ci/qa/phpcpd",
    "phpcbf": "./ci/qa/phpcbf",
    "phpmd": "./ci/qa/phpmd",
    "phpstan": "./ci/qa/phpstan",
    "phpstan-baseline": "./ci/qa/phpstan-update-baseline",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "@auto-scripts"
    ]
  },
  "archive": {
    "exclude": [
      "!/.env",
      "!/.env.local.php",
      "!/var/cache/prod",
      "!/public/build",
      "/.github",
      "/.gitignore",
      "/symfony.lock",
      "/node_modules",
      "/dev",
      "/bin/phpunit",
      "/translations/sp",
      "/coverage",
      "/docs",
      "tests",
      "/build",
      "/tsconfig.json",
      "/tslint.json",
      "/webpack.config.js",
      "/jest.config.js",
      "/.docheader",
      "/.env.ci",
      "/.env.test",
      "/.env.vm",
      "/*.xml",
      "/*.xml.dist",
      "/config/routes/dev",
      "/config/packages/dev",
      "/config/packages/test",
      "/node_modules",
      "/coverage",
      "/build",
      "/ci",
      "/docker",
      "/var/log",
      "/var/sessions"
    ]
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.2"
    },
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    },
    "incenteev-parameters": [
      {
        "file": "config/packages/parameters.yaml"
      },
      {
        "file": "config/packages/institutions.yaml"
      }
    ]
  }
}
